install.packages(c("effectsize", "farver", "fastmap", "insight", "parameters", "ragg", "systemfonts", "xfun"))
install.packages("oaxaca")
# Criação de um conjunto de dados fictício
set.seed(123)
data <- data.frame(
salary = c(rnorm(100, mean=50, sd=10), rnorm(100, mean=45, sd=10)),
education = c(rnorm(100, mean=15, sd=2), rnorm(100, mean=14, sd=2)),
experience = c(rnorm(100, mean=10, sd=3), rnorm(100, mean=9, sd=3)),
gender = rep(c("male", "female"), each=100)
)
# Ajuste do modelo Oaxaca-Blinder
results <- oaxaca(salary ~ education + experience | gender, data = data)
library(oaxaca)
set.seed(123)
data <- data.frame(
salary = c(rnorm(100, mean=50, sd=10), rnorm(100, mean=45, sd=10)),
education = c(rnorm(100, mean=15, sd=2), rnorm(100, mean=14, sd=2)),
experience = c(rnorm(100, mean=10, sd=3), rnorm(100, mean=9, sd=3)),
gender = rep(c("male", "female"), each=100)
)
# Ajuste do modelo Oaxaca-Blinder
results <- oaxaca(salary ~ education + experience | gender, data = data)
# Visualização dos resultados
summary(results)
data <- data.frame(
salary = c(rnorm(100, mean=50, sd=10), rnorm(100, mean=45, sd=10)),
education = c(rnorm(100, mean=15, sd=2), rnorm(100, mean=14, sd=2)),
experience = c(rnorm(100, mean=10, sd=3), rnorm(100, mean=9, sd=3)),
gender = rep(c("male", "female"), each=100)
)
# Ajuste do modelo Oaxaca-Blinder
results <- oaxaca(salary ~ education + experience | gender, data = data)
data$gender_binary <- ifelse(data$gender == "male", 1, 0)
# Ajuste do modelo Oaxaca-Blinder
results <- oaxaca(salary ~ education + experience | gender_binary, data = data)
# Visualização dos resultados
summary(results)
